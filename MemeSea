local autoDrop = false
local autoStore = false

local function pegarPowersDoBackpack()
	local backpack = player:FindFirstChild(BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('MWkvVyqTmNqiPizKnWRCHOczdbVLeuWpcxxSOehbVtmpPJRLKChCIbUQmFja3BhY2s='))
	if not backpack then return {} end

	local list = {}
	for _, obj in ipairs(backpack:GetChildren()) do
		if obj.Name:find(BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('ZIOzMOducdxrcVpzlonlMKNUdOHMqHAyavmbOJBMJDrVHFGzqVdYqTRUG93ZXI=')) then
			table.insert(list, obj)
		end
	end
	return list
end

local function pegarPowersDoCharacter()
	local char = player.Character
	if not char then return {} end

	local list = {}
	for _, obj in ipairs(char:GetChildren()) do
		if obj.Name:find(BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('DCMTDHIJMsFcGNxoCbXEbPqYRzrFmmGouWwVOxKJCjhmWwlIFVlvEorUG93ZXI=')) then
			table.insert(list, obj)
		end
	end
	return list
end

local function moverParaCharacter()
	local char = player.Character
	if not char then return end

	for _, power in ipairs(pegarPowersDoBackpack()) do
		power.Parent = char
	end
end

local function droparPowers()
	for _, power in ipairs(pegarPowersDoCharacter()) do
		PowerRemote:FireServer(BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('EYCzRCSvCVGtJoeDqZjVhHJETTSpALxXZBgHDLycLQTIoDIkOEOKmOSRWF0YWJsZV9Qb3dlcg=='), {
			Action = BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('kgBCOHMEfSqxvhgfcqNUVgdOqpBsVVjiIdZORzcKjWImXrNAvAZSeoiRHJvcA=='),
			Tool = power
		})
	end
end

local function guardarPowers()
	for _, power in ipairs(pegarPowersDoCharacter()) do
		PowerRemote:FireServer(BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('WbBcDYtsoPRiLnLWntmoZTDPkRZHeWrRtdTkRwPMqnhzlkATtOFPhOMRWF0YWJsZV9Qb3dlcg=='), {
			Action = BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('wMKtQNLRMRABvAUURGNEgZomoWApUMOXWnbeyfpGPpCRvzeLETViNThU3RvcmU='),
			Tool = power
		})
	end
end

task.spawn(function()
	while true do
		if autoDrop then
			moverParaCharacter()
			droparPowers()
		end

		if autoStore then
			moverParaCharacter()
			guardarPowers()
		end

		task.wait(0.05)
	end
end)

dropBtn.MouseButton1Click:Connect(function()
	autoDrop = not autoDrop
	if autoDrop then
		dropBtn.Text = BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('XZItDKfLqwvRDjiAzYEJCHGsghoKJbqjnWBommvsxHwvbHiwHOFZmccRGVzYXRpdmFyIEF1dG8gRHJvcA==')
	else
		dropBtn.Text = BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('LhwOENdcktoDFgkJZRddOboopxCpDYXYOYzWqYzlTAjqzXWhpNvLhaLQXRpdmFyIEF1dG8gRHJvcA==')
	end
end)

storeBtn.MouseButton1Click:Connect(function()
	autoStore = not autoStore
	if autoStore then
		storeBtn.Text = BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('iklcLBqiaiIMJjGjfFaGEkXpZRvwLbUFRquUXputJNIQQHQiXUKBoDHRGVzYXRpdmFyIEF1dG8gU3RvcmU=')
	else
		storeBtn.Text = BfDzCdyaoMGsjstdoGVeSxDsWqSbtuHnPpAeqESxdMHGgFySKyKxIZVWtndhQCG('DyvsJLTiOXgYNBSxxaoVWeixVWjBaYxMPzZVZVPyuMMWgtrmtlLzwGbQXRpdmFyIEF1dG8gU3RvcmU=')
	end
end)

local originalSize = frame.Size

minimizeBtn.MouseButton1Click:Connect(function()
	frame.Size = UDim2.new(0, 260, 0, 40)
	dropBtn.Visible = false
	storeBtn.Visible = false
	minimizeBtn.Visible = false
	expandBtn.Visible = true
end)

expandBtn.MouseButton1Click:Connect(function()
	frame.Size = originalSize
	dropBtn.Visible = true
	storeBtn.Visible = true
	minimizeBtn.Visible = true
	expandBtn.Visible = false
end)
    
