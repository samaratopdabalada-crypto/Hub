-- GUI arrast√°vel + Voo + Kill Aura + Slider de raio
-- Feito para executores (Delta, Hydrogen, etc)

-- GUI principal
local Player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui", Player:WaitForChild("PlayerGui"))
gui.Name = "ExploitGUI"

-- Emoji arrast√°vel
local emojiBtn = Instance.new("TextButton")
emojiBtn.Size = UDim2.new(0, 60, 0, 60)
emojiBtn.Position = UDim2.new(0.1, 0, 0.1, 0)
emojiBtn.BackgroundTransparency = 1
emojiBtn.Text = "ü§ë"
emojiBtn.TextScaled = true
emojiBtn.Name = "ArrastavelEmoji"
emojiBtn.Parent = gui

-- Drag and drop
local dragging, dragInput, dragStart, startPos
emojiBtn.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = emojiBtn.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)
emojiBtn.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)
game:GetService("UserInputService").InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        local delta = input.Position - dragStart
        emojiBtn.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- GUI frame
local apelaoFrame = Instance.new("Frame")
apelaoFrame.Size = UDim2.new(0, 220, 0, 240)
apelaoFrame.Position = UDim2.new(0.5, -110, 0.5, -120)
apelaoFrame.BackgroundColor3 = Color3.fromRGB(40,40,40)
apelaoFrame.Visible = false
apelaoFrame.Name = "apelao"
apelaoFrame.Parent = gui

local titulo = Instance.new("TextLabel")
titulo.Size = UDim2.new(1,0,0.2,0)
titulo.Position = UDim2.new(0,0,0,0)
titulo.BackgroundTransparency = 1
titulo.Text = "GUI APELAO"
titulo.TextColor3 = Color3.fromRGB(255,255,127)
titulo.TextScaled = true
titulo.Parent = apelaoFrame

-- Bot√£o voo
local botao = Instance.new("TextButton")
botao.Size = UDim2.new(0.8,0,0.18,0)
botao.Position = UDim2.new(0.1,0,0.22,0)
botao.Text = "Ativar Voo"
botao.BackgroundColor3 = Color3.fromRGB(0,170,0)
botao.TextColor3 = Color3.new(1,1,1)
botao.TextScaled = true
botao.Parent = apelaoFrame

-- Bot√£o Kill Aura
local killAuraBtn = Instance.new("TextButton")
killAuraBtn.Size = UDim2.new(0.8,0,0.18,0)
killAuraBtn.Position = UDim2.new(0.1,0,0.42,0)
killAuraBtn.Text = "Kill Aura: OFF"
killAuraBtn.BackgroundColor3 = Color3.fromRGB(170,170,0)
killAuraBtn.TextColor3 = Color3.new(1,1,1)
killAuraBtn.TextScaled = true
killAuraBtn.Parent = apelaoFrame

-- Slider de raio
local radiusLabel = Instance.new("TextLabel")
radiusLabel.Size = UDim2.new(0.8,0,0.13,0)
radiusLabel.Position = UDim2.new(0.1,0,0.62,0)
radiusLabel.BackgroundTransparency = 1
radiusLabel.Text = "Kill Aura Radius: 10"
radiusLabel.TextColor3 = Color3.new(1,1,1)
radiusLabel.TextScaled = true
radiusLabel.Parent = apelaoFrame

local sliderBar = Instance.new("Frame")
sliderBar.Size = UDim2.new(0.8,0,0.06,0)
sliderBar.Position = UDim2.new(0.1,0,0.77,0)
sliderBar.BackgroundColor3 = Color3.fromRGB(100,100,120)
sliderBar.BorderSizePixel = 0
sliderBar.Parent = apelaoFrame

local sliderBtn = Instance.new("TextButton")
sliderBtn.Size = UDim2.new(0,20,1,0)
sliderBtn.Position = UDim2.new(0,0,0,0)
sliderBtn.BackgroundColor3 = Color3.fromRGB(220,220,0)
sliderBtn.Text = ""
sliderBtn.Parent = sliderBar

local minRadius, maxRadius = 1, 100
local killAuraRadius = 10

-- Slider funcional
local sliding = false
sliderBtn.MouseButton1Down:Connect(function()
    sliding = true
end)
game:GetService("UserInputService").InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        sliding = false
    end
end)
sliderBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        sliding = true
    end
end)
sliderBar.InputChanged:Connect(function(input)
    if sliding and input.UserInputType == Enum.UserInputType.MouseMovement then
        local barAbsPos = sliderBar.AbsolutePosition.X
        local barAbsSize = sliderBar.AbsoluteSize.X
        local mousePos = input.Position.X
        local percent = math.clamp((mousePos - barAbsPos) / barAbsSize, 0, 1)
        sliderBtn.Position = UDim2.new(percent, -10, 0, 0)
        killAuraRadius = math.floor(minRadius + percent * (maxRadius-minRadius))
        radiusLabel.Text = "Kill Aura Radius: " .. tostring(killAuraRadius)
    end
end)
sliderBtn.MouseMoved:Connect(function(x, y)
    if sliding then
        local barAbsPos = sliderBar.AbsolutePosition.X
        local barAbsSize = sliderBar.AbsoluteSize.X
        local percent = math.clamp((x - barAbsPos) / barAbsSize, 0, 1)
        sliderBtn.Position = UDim2.new(percent, -10, 0, 0)
        killAuraRadius = math.floor(minRadius + percent * (maxRadius-minRadius))
        radiusLabel.Text = "Kill Aura Radius: " .. tostring(killAuraRadius)
    end
end)

-- Abrir GUI
emojiBtn.MouseButton1Click:Connect(function()
    apelaoFrame.Visible = not apelaoFrame.Visible
end)

-- Voo
local flying = false
local bp, bg
local SPEED = 50

local function startFly()
    local char = Player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end

    bp = Instance.new("BodyVelocity")
    bp.MaxForce = Vector3.new(1e5, 1e5, 1e5)
    bp.Velocity = Vector3.new(0,0,0)
    bp.Parent = char.HumanoidRootPart

    bg = Instance.new("BodyGyro")
    bg.MaxTorque = Vector3.new(1e9, 1e9, 1e9)
    bg.Parent = char.HumanoidRootPart

    flying = true

    spawn(function()
        while flying and bp and char and char:FindFirstChild("HumanoidRootPart") do
            local camera = workspace.CurrentCamera
            bp.Velocity = camera.CFrame.LookVector * SPEED
            wait(0.01)
        end
    end)
end
local function stopFly()
    flying = false
    local char = Player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        if char.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity") then
            char.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity"):Destroy()
        end
        if char.HumanoidRootPart:FindFirstChildOfClass("BodyGyro") then
            char.HumanoidRootPart:FindFirstChildOfClass("BodyGyro"):Destroy()
        end
    end
end
botao.MouseButton1Click:Connect(function()
    if not flying then
        startFly()
        botao.Text = "Desativar Voo"
        botao.BackgroundColor3 = Color3.fromRGB(170,0,0)
    else
        stopFly()
        botao.Text = "Ativar Voo"
        botao.BackgroundColor3 = Color3.fromRGB(0,170,0)
    end
end)

-- Kill Aura
local killAuraActive = false
local killAuraPart

local function createKillAuraPart()
    if killAuraPart then killAuraPart:Destroy() end
    local char = Player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    killAuraPart = Instance.new("Part")
    killAuraPart.Shape = Enum.PartType.Ball
    killAuraPart.Anchored = true
    killAuraPart.CanCollide = false
    killAuraPart.Size = Vector3.new(killAuraRadius*2, killAuraRadius*2, killAuraRadius*2)
    killAuraPart.CFrame = char.HumanoidRootPart.CFrame
    killAuraPart.Transparency = 0.7
    killAuraPart.Color = Color3.fromRGB(255,0,0)
    killAuraPart.Parent = workspace
end

local function updateKillAuraPart()
    if killAuraPart then
        local char = Player.Character
        if not char or not char:FindFirstChild("HumanoidRootPart") then return end
        killAuraPart.Size = Vector3.new(killAuraRadius*2, killAuraRadius*2, killAuraRadius*2)
        killAuraPart.CFrame = char.HumanoidRootPart.CFrame
    end
end

local function destroyKillAuraPart()
    if killAuraPart then
        killAuraPart:Destroy()
        killAuraPart = nil
    end
end

local function dealKillAuraDamage()
    local char = Player.Character
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    for _, model in ipairs(workspace:GetChildren()) do
        if model:IsA("Model") and model ~= char and model:FindFirstChild("Humanoid") and model:FindFirstChild("HumanoidRootPart") then
            local dist = (model.HumanoidRootPart.Position - char.HumanoidRootPart.Position).Magnitude
            if dist <= killAuraRadius then
                -- Tentar dar dano (s√≥ funciona se o servidor permitir)
                pcall(function()
                    model.Humanoid:TakeDamage(10)
                end)
            end
        end
    end
end

spawn(function()
    while true do
        if killAuraActive then
            createKillAuraPart()
            updateKillAuraPart()
            dealKillAuraDamage()
        else
            destroyKillAuraPart()
        end
        wait(0.4)
    end
end)

killAuraBtn.MouseButton1Click:Connect(function()
    killAuraActive = not killAuraActive
    if killAuraActive then
        killAuraBtn.Text = "Kill Aura: ON"
        killAuraBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    else
        killAuraBtn.Text = "Kill Aura: OFF"
        killAuraBtn.BackgroundColor3 = Color3.fromRGB(170,170,0)
        destroyKillAuraPart()
    end
end)

sliderBtn.MouseButton1Down:Connect(function()
    sliding = true
    spawn(function()
        while sliding do
            createKillAuraPart()
            updateKillAuraPart()
            wait(0.1)
        end
    end)
end)
