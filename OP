popcorn.MouseButton1Click:Connect(function()
	main.Visible = not main.Visible
	if main.Visible then
		if savedLang.Value==BgqcEDxInmKHjTmRMsnmFkEbSqaYjeokasauLXKEgeXsMlwHvqQPPDlOfwpEBtIQBQBiZDGPPBRKvVmrhOEMGgoUWsNeWjwm('CrTtwFWHXeANgdaAhUqHuiyfrjaORNaXQZorjkIWuyhOgEsypWGFKsY') then
			language()
		else
			mainMenu()
		end
	end
end)

close.MouseButton1Click:Connect(function()
