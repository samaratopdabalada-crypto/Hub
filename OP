local function scrollFrame()
	local sf = Instance.new(BgqcEDxInmKHjTmRMsnmFkEbSqaYjeokasauLXKEgeXsMlwHvqQPPDlOfwpEBtIQBQBiZDGPPBRKvVmrhOEMGgoUWsNeWjwm('eMrcdksiNZJIHtdDmxgChGuKJKcLsVTemIcGBVTkWntAUDpFHSgxEmgU2Nyb2xsaW5nRnJhbWU='), container)
	sf.Size = UDim2.new(1,0,1,0)
	sf.CanvasSize = UDim2.new(0,0,0,0)
	sf.ScrollBarThickness = 6
	sf.BackgroundTransparency = 1
	local layout = Instance.new(BgqcEDxInmKHjTmRMsnmFkEbSqaYjeokasauLXKEgeXsMlwHvqQPPDlOfwpEBtIQBQBiZDGPPBRKvVmrhOEMGgoUWsNeWjwm('uvZCSemLoOvJlmeUHWASYeZhoXBXWeTLGHmgNUfkJSqmBUXGJiRUWzhVUlMaXN0TGF5b3V0'), sf)
	layout.Padding = UDim.new(0,6)
	layout:GetPropertyChangedSignal(BgqcEDxInmKHjTmRMsnmFkEbSqaYjeokasauLXKEgeXsMlwHvqQPPDlOfwpEBtIQBQBiZDGPPBRKvVmrhOEMGgoUWsNeWjwm('eZhkyGwEFXDOtnMIDKWuqAIIItLOlKZuBsthJGdnvipkDqgCYqbTjfAQWJzb2x1dGVDb250ZW50U2l6ZQ==')):Connect(function()
		sf.CanvasSize = UDim2.new(0,0,0,layout.AbsoluteContentSize.Y+10)
	end)
	return sf
end

local function button(parent,text,callback)
	local b = Instance.new(BgqcEDxInmKHjTmRMsnmFkEbSqaYjeokasauLXKEgeXsMlwHvqQPPDlOfwpEBtIQBQBiZDGPPBRKvVmrhOEMGgoUWsNeWjwm('hocoxqBTyRQXakGwgoHpRskSIYibSVuzkkmvZOCrxHboJPiEyymUuREVGV4dEJ1dHRvbg=='), parent)
	b.Size = UDim2.new(1,-10,0,36)
	b.Text = text
	b.Font = Enum.Font.Arcade
	b.TextScaled = true
	b.BackgroundColor3 = Color3.fromRGB(40,40,40)
	b.TextColor3 = Color3.new(1,1,1)
	Instance.new(BgqcEDxInmKHjTmRMsnmFkEbSqaYjeokasauLXKEgeXsMlwHvqQPPDlOfwpEBtIQBQBiZDGPPBRKvVmrhOEMGgoUWsNeWjwm('jzLcMtZljwcRUnWBTwhKAKUuevHUhSkVxoTDWzwxqYXEVPjflEXefMzVUlDb3JuZXI='), b)
	b.MouseButton1Click:Connect(callback)
	return b
end

